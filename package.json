{
  "name": "angular-experiments",
  "version": "0.0.0",
  "license": "",
  "author": "Matt Morris <matt.m.morris0@gmail.com",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build -prod -aot --build-optimizer",
    "install-firebase-functions": "cd functions && yarn install",
    "test": "ng test",
    "test:ci": "ng test --watch=false --code-coverage",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "codecov": "codecov",
    "bundlesize": "bundlesize",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "5.0.0-beta.4",
    "@angular/compiler": "5.0.0-beta.4",
    "@angular/core": "5.0.0-beta.4",
    "@angular/forms": "5.0.0-beta.4",
    "@angular/http": "5.0.0-beta.4",
    "@angular/platform-browser": "5.0.0-beta.4",
    "@angular/platform-browser-dynamic": "5.0.0-beta.4",
    "@angular/router": "5.0.0-beta.4",
    "bundlesize": "^0.14.2",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "1.4.0-rc.1",
    "@angular/compiler-cli": "5.0.0-beta.4",
    "@types/jasmine": "2.5.54",
    "@types/node": "~8.0.25",
    "codecov": "^2.1.0",
    "codelyzer": "~3.1.2",
    "coveralls": "^2.13.0",
    "firebase-tools": "^3.6.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.1"
  },
  "bundlesize": [
    {
      "path": "./dist/main.*.js",
      "maxSize": "70 kB"
    },
    {
      "path": "./dist/inline.*.js",
      "maxSize": "1 kB"
    },
    {
      "path": "./dist/polyfills.*.js",
      "maxSize": "21 kB"
    },
    {
      "path": "./dist/?(1|2|3|4|6|7|8|9|0).*.js",
      "maxSize": "5 kB"
    }
  ]
}
