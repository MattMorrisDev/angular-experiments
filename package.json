{
  "name": "angular-experiments",
  "version": "0.0.0",
  "license": "",
  "author": "Matt Morris <matt.m.morris0@gmail.com",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build -prod -aot --build-optimizer",
    "install-firebase-functions": "cd functions && yarn install",
    "test": "ng test",
    "test:ci": "ng test --watch=false --code-coverage",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "codecov": "codecov",
    "bundlesize": "bundlesize",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0-beta.3",
    "@angular/cdk": "5.2.0",
    "@angular/common": "6.0.0-beta.3",
    "@angular/compiler": "6.0.0-beta.3",
    "@angular/core": "6.0.0-beta.3",
    "@angular/forms": "6.0.0-beta.3",
    "@angular/http": "6.0.0-beta.3",
    "@angular/material": "5.2.0",
    "@angular/platform-browser": "6.0.0-beta.3",
    "@angular/platform-browser-dynamic": "6.0.0-beta.3",
    "@angular/router": "6.0.0-beta.3",
    "bundlesize": "^0.14.2",
    "core-js": "2.5.1",
    "rxjs": "5.5.6",
    "zone.js": "0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "7.1.4",
    "@angular/compiler-cli": "6.0.0-beta.3",
    "@angular/language-service": "6.0.0-beta.3",
    "@types/jasmine": "2.5.54",
    "@types/node": "~8.0.25",
    "codecov": "^2.1.0",
    "codelyzer": "~4.0.0",
    "coveralls": "^2.13.0",
    "firebase-tools": "^3.6.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.6.2"
  },
  "bundlesize": [
    {
      "path": "./dist/main.*.js",
      "maxSize": "70 kB"
    },
    {
      "path": "./dist/inline.*.js",
      "maxSize": "1 kB"
    },
    {
      "path": "./dist/polyfills.*.js",
      "maxSize": "22 kB"
    }
  ]
